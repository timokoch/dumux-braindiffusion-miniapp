[TimeLoop]
TEnd = 72h
InitialTimeStepSize = 1h

[Grid]
File = ../../data/mesh-data/mri_processed_data/sub-01/modeling/resolution32/data.vtu
Verbosity = true

[Problem]
Name = diffusion
SurfaceConductance = 0.5e-4 # in mm/s
TransferCoeffToBlood = 0.1e-6 # in 1/s
PorosityBrain = 0.22
PorositySAS = 0.55
DiffusionType = DataTensor
UseCurveFit = true
DiffusionFactor = 1.0

[LevenbergMarquardt]
BaseEpsilon = 1e-3
